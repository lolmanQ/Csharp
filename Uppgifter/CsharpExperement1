using System;

class MainClass {
  static void Main () {
		int inputInt = 0;
		while(!int.TryParse(Console.ReadLine(),out inputInt)){
				Console.WriteLine("invalid");
		}
		Console.WriteLine("Multiplicationstabelen f√∂r " + inputInt);
		for(int i = 1; i<11; i++){
			Console.WriteLine(inputInt + "*" + i + " = " + inputInt * i);
		}
  }
}
